RewriteRule .* - [L]

# docomo
RewriteCond %{HTTP_USER_AGENT} DoCoMo
RewriteRule .* - [E=HTTP_UA:docomo]

# softbank
RewriteCond %{HTTP_USER_AGENT} J-PHONE [OR]
RewriteCond %{HTTP_USER_AGENT} Vodafone [OR]
