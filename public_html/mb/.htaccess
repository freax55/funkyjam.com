RewriteRule .* - [E=HTTP_UA:softbank]

# au
RewriteCond %{HTTP_USER_AGENT} KDDI
RewriteCond %{HTTP_USER_AGENT} UP.Browser
RewriteRule .* - [E=HTTP_UA:au]

# other
RewriteCond %{ENV:HTTP_UA} ^$
RewriteRule .* - [E=HTTP_UA:other]

